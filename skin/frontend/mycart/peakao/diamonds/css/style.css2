.complete-ring-container{
padding: 10px 30px;
font-size: 20px;
color:#8e80ac;
}
.complete-diamond-image {
    position: absolute;
    left: 12px;
    top: 12px;
    z-index: 10;
}
.complete-diamond-image a {
    width: 60px;
    height: 60px;
    border: 1px solid #fff;
    margin-right: 10px;
    display: inline-block;
}
.complete-ring-part .RingSmallImg {
    float: left;
    width: 16px;
    height: 20px;
    background-image: url('data:image/png;base64,');
    margin-right: 10px;
}
.complete-ring-part .DiamondSmallImg {
    float: left;
    width: 19px;
    height: 16px;
    background-image: url('data:image/png;base64,');
    margin-right: 7px;
    margin-top: 4px;
}
#constructor .shape img{
cursor: pointer;
} 
#constructor .shape.active  img{
box-shadow:
3px 3px 3px #8e80ac inset,

3px 3px 3px #8e80ac;

}